<?xml version="1.0"?>
<glade-catalog name="abroun_gtk_gui_widgets" library="gladepython" domain="glade-3" depends="gtk+">
  <init-function>glade_python_init</init-function>
  <glade-widget-classes>
    <glade-widget-class name="FilePath"
                        generic-name="filePath"
                        title="File Path" />
    <glade-widget-class name="SequenceControl"
                        generic-name="sequenceControl"
                        title="Sequence Control" />
    <glade-widget-class name="MinMaxSlider"
                        generic-name="slider"
                        title="MinMax Slider" />
  </glade-widget-classes>

  <glade-widget-group name="abroun_widgets" title="abroun Widgets">

    <glade-widget-class-ref name="FilePath"/>
    <glade-widget-class-ref name="SequenceControl"/>
    <glade-widget-class-ref name="MinMaxSlider"/>

  </glade-widget-group>
  
</glade-catalog>
