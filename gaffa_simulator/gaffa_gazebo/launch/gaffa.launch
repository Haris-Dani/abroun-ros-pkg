<launch>
  
  <!-- startup gaffa without starting up any defaut controllers -->
  <include file="$(find gaffa_gazebo)/launch/gaffa_no_controllers.launch" />

  <!-- Default Controllers -->
  <include file="$(find gaffa_gazebo)/launch/gaffa_default_controllers.launch" />

  <!-- Simulated SSC32 Server -->
  <include file="$(find gaffa_gazebo)/launch/simulated_ssc32_server.launch" />

</launch>

